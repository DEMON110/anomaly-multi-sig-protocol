# RPC endpoints (use any one you prefer)
GOERLI_RPC_URL=https://eth-goerli.example/your-key
SEPOLIA_RPC_URL=https://eth-sepolia.example/your-key
HOLESKY_RPC_URL=https://holesky.example/your-key

# Deployer private key (never commit your real key)
PRIVATE_KEY=0xabc123...deadbeef

# Optional for verification
ETHERSCAN_API_KEY=your-key

# Deployment parameters
# Comma-separated list of EOA addresses as initial signers (e.g., 3-of-2 threshold)
SIGNERS=0xSigner1,0xSigner2,0xSigner3
THRESHOLD=2
